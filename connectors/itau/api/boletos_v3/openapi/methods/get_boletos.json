{
    "tags": [
        "boletos"
    ],
    "operationId": "get/boletos",
    "description": "Consulta lista de boletos por critérios específicos.",
    "parameters": [
        {
            "$ref": "#/components/parameters/x-itau-apikey"
        },
        {
            "$ref": "#/components/parameters/x-itau-correlationid"
        },
        {
            "in": "header",
            "name": "x-itau-flowid",
            "description": "Key que identifica o fluxo",
            "schema": {
                "type": "string"
            },
            "required": false
        },
        {
            "in": "query",
            "name": "codigo_carteira",
            "description": "Número da carteira utilizada na emissão do boleto",
            "schema": {
                "type": "string"
            },
            "required": false
        },
        {
            "in": "query",
            "name": "data_inicial",
            "description": "Data inicial da pesquisa",
            "example": "2021-01-23T00:00:00.000Z",
            "schema": {
                "type": "string"
            },
            "required": true
        },
        {
            "in": "query",
            "name": "data_final",
            "description": "Data final da pesquisa",
            "example": "2021-01-23T00:00:00.000Z",
            "schema": {
                "type": "string"
            },
            "required": true
        },
        {
            "in": "query",
            "name": "id_beneficiario",
            "description": "(4 dígitos agência)(5 dígitos c/c)+(1 dígito DAC)",
            "example": "1500010044",
            "schema": {
                "type": "string"
            },
            "required": true
        },
        {
            "in": "query",
            "name": "nome_pagador",
            "description": "Nome do destinatário do boleto, pode ser informado apenas uma parte do nome para pesquisa, todavia, será pesquisado somente a frente do nome",
            "schema": {
                "type": "string"
            },
            "required": false
        },
        {
            "in": "query",
            "name": "numero_cpf_cnpj_pagador",
            "description": "Número do CPF ou CNPJ do destinatário do boleto. Deve ser informado apenas números",
            "schema": {
                "type": "number"
            },
            "required": false
        },
        {
            "in": "query",
            "name": "numero_nosso_numero",
            "description": "Nosso número informado na emissão do boleto, importante, sempre o último digito será considerado como DAC",
            "schema": {
                "type": "number"
            },
            "required": false
        },
        {
            "in": "query",
            "name": "seu_numero",
            "description": "Seu número, informado pelo beneficiário de cobrança, do boleto de cobrança, provido do processor-boleto.",
            "example": "1234",
            "schema": {
                "type": "string",
                "maxLength": 14
            },
            "required": false
        },
        {
            "in": "query",
            "name": "situacao_geral_boleto",
            "description": "Status do boleto a ser filtrado. São aceitos somente os valores (chave) retornados no end-point de configuração na chave 'filtro_situacao_geral_boleto'.",
            "schema": {
                "type": "string"
            },
            "required": false
        },
        {
            "in": "query",
            "name": "situacao_negativacao",
            "description": "Informa a situação da negativação do boleto. São aceitos somente os valores (chave) retornados no end-point de configuração na chave 'filtro_situacao_negativacao'.",
            "schema": {
                "type": "string"
            },
            "required": false
        },
        {
            "in": "query",
            "name": "situacao_protesto",
            "description": "Informa a situação do protesto do boleto. São aceitos somente os valores (chave) retornados no end-point de configuração na chave 'filtro_situacao_protesto'.",
            "schema": {
                "type": "string"
            },
            "required": false
        },
        {
            "in": "query",
            "name": "status_vencimento",
            "description": "São aceitos somente os valores (chave) retornados no end-point de configuração na chave 'filtro_status_vencimento'.",
            "schema": {
                "type": "string"
            },
            "required": false
        },
        {
            "in": "query",
            "name": "tipo_boleto",
            "description": "Informa o tipo de boleto a ser pesquisado. São aceitos somente os valores (chave) retornados no end-point de configuração na chave 'filtro_tipo_boleto'.",
            "schema": {
                "type": "string"
            },
            "required": false
        },
        {
            "in": "query",
            "name": "tipo_data",
            "description": "Campo que indica onde a pesquisa será realizada, na data de emissão ou na data de vencimento. São aceitos somente os valores (chave) retornados no end-point de configuração na chave 'filtro_tipo_data'.",
            "schema": {
                "type": "string"
            },
            "required": true
        },
        {
            "in": "query",
            "name": "page",
            "description": "Definir um número de página para obter informações desta coleção",
            "example": 5,
            "schema": {
                "type": "integer"
            },
            "required": false
        },
        {
            "in": "query",
            "name": "page_size",
            "description": "Limitar o número de elementos por página",
            "example": 20,
            "schema": {
                "type": "integer"
            },
            "required": false
        }
    ],
